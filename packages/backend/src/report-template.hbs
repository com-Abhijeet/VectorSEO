    <script>
    // Chart.js script for the overall score donut chart
    const score = {{scores.overallScore}};
    let scoreColor;

    // New logic to determine the color based on the score
    if (score >= 85) {
        scoreColor = '#16a34a'; // Green for 85-100
    } else if (score >= 70) {
        scoreColor = '#f59e0b'; // Yellow for 70-84
    } else if (score >= 50) {
        scoreColor = '#f97316'; // Orange for 50-69
    } else {
        scoreColor = '#dc2626'; // Red for below 50
    }

    const ctx = document.getElementById('seoScoreChart').getContext('2d');
    const seoScoreChart = new Chart(ctx, {
        type: 'doughnut',
        data: {
            datasets: [{
                data: [score, 100 - score],
                backgroundColor: [scoreColor, '#e2e8f0'], // Uses the dynamically set scoreColor
                borderWidth: 0,
                borderRadius: 5,
            }]
        },
            options: {
                responsive: true,
                cutout: '75%',
                plugins: {
                    legend: { display: false },
                    tooltip: { enabled: false }
                },
                animation: {
                    animateRotate: true
                }
            }
        });
    </script>
